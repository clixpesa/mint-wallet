{
  "expo": {
    "name": "Clixpesa Dev",
    "slug": "clixpesa-mint",
    "owner": "clixpesa",
    "version": "2.0.2",
    "orientation": "portrait",
    "icon": "./ui/assets/images/icon.png",
    "scheme": "clix.eth",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./ui/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.clixpesa.app.test",
      "googleServicesFile": "./google-services.test.json"
    },
    
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./ui/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./ui/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./ui/assets/fonts/Inter-Medium.ttf",
            "./ui/assets/fonts/Inter-Regular.ttf",
            "./ui/assets/fonts/Inter-SemiBold.ttf",
            "./ui/assets/fonts/Inter-Bold.ttf",
            "./ui/assets/fonts/InputMono-Regular.ttf",
            "./ui/assets/fonts/SpaceMono-Regular.ttf"
          ]
        }
      ],
      "expo-secure-store",
      [
        "react-native-bottom-tabs",
        {
          "theme": "material3"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b6c6e91a-96d7-47ea-97be-9f396c1f0710"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b6c6e91a-96d7-47ea-97be-9f396c1f0710"
    }
  }
}
